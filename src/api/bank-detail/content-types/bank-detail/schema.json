{
  "kind": "collectionType",
  "collectionName": "bank_details",
  "info": {
    "singularName": "bank-detail",
    "pluralName": "bank-details",
    "displayName": "BankDetails",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bankAccountNumber": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 16,
      "minLength": 12
    },
    "bankAccountLabel": {
      "type": "string",
      "required": true
    },
    "bankAccountHolderName": {
      "type": "string",
      "required": true
    },
    "bankName": {
      "type": "enumeration",
      "enum": [
        "Agriculture Development Bank",
        "Nepal Bank",
        "Rastriya Banijya Bank",
        "Citizens Bank International Limited",
        "Everest Bank",
        "Global IME Bank Limited",
        "Himalayan Bank",
        "Kumari Bank",
        "Laxmi Sunrise Bank",
        "Machhapuchhre Bank",
        "Nabil Bank",
        "Nepal Investment Mega Bank",
        "Nepal SBI Bank",
        "NIC Asia Bank",
        "NMB Bank",
        "Prabhu Bank",
        "Prime Commercial Bank",
        "Sanima Bank",
        "Siddhartha Bank",
        "Standard Chartered Bank"
      ]
    },
    "branchName": {
      "type": "string",
      "required": true
    },
    "employee": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::employee.employee",
      "mappedBy": "secondary_bank_detail"
    }
  }
}
